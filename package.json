    "@aomao/plugin-codeblock": "^1.3.0",
    "@aomao/plugin-fontcolor": "^1.3.0",
    "@aomao/plugin-fontsize": "^1.3.0",
    "@aomao/plugin-heading": "^1.3.0",
    "@aomao/plugin-hr": "^1.3.0",
    "@aomao/plugin-image": "^1.3.0",
